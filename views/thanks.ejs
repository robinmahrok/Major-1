<script>

function stringGen() {
  var text = "";
  var char_list =
    "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789";
  for (var i = 0; i < 4; i++) {
    text += char_list.charAt(Math.floor(Math.random() * char_list.length));
  }
  document.getElementById("result").innerHTML = text;
  return text;
}
</script>
